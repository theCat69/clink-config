ex=explorer .
vim=nvim $*
jv=bf-j-vm $*
docker=podman $*
ij=idea64 .
gd=cd %DEV%
cdi=zi
grep=rg $*
ls=eza $*
ll=eza -la
lsl=eza -la --sort oldest
cat=bat -p $*
gp=git push $*
gpl=git pull $*
gplr=git pull --rebase $*
gr=git rebase $*
gm=git mergetool $*
gd=git diff $*
grm=git rm $*
ga=git add . $*
gc=git commit -m $*
gi=git init $*
gb=git branch -a $*
